{
  "configurations": [
    {
      "name": "Win32",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/.esphome/build/**/src/**",
        "${userProfile}/.platformio/packages/framework-espidf/components/**",
        "${userProfile}/.platformio/packages/toolchain-*/*/include/**"
      ],
      "browse": {
        "path": [
          "${workspaceFolder}",
          "${userProfile}/.platformio/packages/framework-espidf"
        ],
        "limitSymbolsToIncludedHeaders": true
      },
      "defines": [
        "ESPHOME_LOG_LEVEL=ESPHOME_LOG_LEVEL_DEBUG",
        "USE_ESP_IDF",
        "USE_ETHERNET",
        "ESP_PLATFORM",
        "IDF_VER=\"v5.1\""
      ],
      "compilerPath": "${userProfile}/.platformio/packages/toolchain-riscv32-esp/bin/riscv32-esp-elf-g++.exe",
      "cStandard": "c11",
      "cppStandard": "gnu++17",
      "intelliSenseMode": "gcc-x64"
    },
    {
